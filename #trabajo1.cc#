//programa que calcula la suma de numeros
//impares del 1-10
//Margarita Vega Galindez
#include <iostream>
using namespace std;

int main()

{
  int sum=0;
  int count=1;
  while(count<20) {
    if((count%2)>0)

      {
	sum=sum+count;
      }
    count++;
  }
    cout<<"suma"<<sum<<endl;
    return 0;
    }
